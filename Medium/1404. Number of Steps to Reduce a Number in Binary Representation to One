class Solution {
    public int numSteps(String s) {
        int res=0;
        int j=s.length()-1;
        int i=j;
        while (s.charAt(i)=='0'){
            i-=1;
        }
        res+=(j-i);
        j=i;
        i-=1;
        while(i>=0){
            while(i>-1 && s.charAt(i)!='0'){
                i-=1;
            }
            res+=1;
            res+=(j-i);
            j=i;
            i-=1;
        }
        return res;

    }
}
